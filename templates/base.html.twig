            {# <link href="/css/baseStyle.css" rel="stylesheet"/> #}
            <link href="{{ asset('css/baseStyle.css') }}" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            {# the JS file lives at "public/js/baseJs.js" #}
            {# <script src="{{ asset('public/js/baseJs.js') }}"></script> #}
            <script src="{{ asset('/js/baseJs.js') }}"></script>
{% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
